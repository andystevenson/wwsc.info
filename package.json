{
  "name": "wwsc.info",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "init": "npm link",
    "dev": "run-s init build-cache serve:11ty",
    "serve:11ty": "npx @11ty/eleventy --serve --incremental",
    "build": "run-s init build-cache production",
    "production": "npx @11ty/eleventy",
    "clean": "rm -rf _site .11ty* .cache/*",
    "clean-cache": "concurrently --timings npm:clean:*",
    "clean:cache-ashbourne-csv": "rm -f .cache/ashbourne/ashbourne.csv",
    "clean:cache-ashbourne-lint": "rm -f .cache/ashbourne/ashbourne-lint*",
    "clean:cache-ashbourne-json": "rm -f .cache/ashbourne/ashbourne.json",
    "clean:cache-stripe-join": "rm -f .cache/stripe/join.json",
    "clean:cache-stripe-subscriptions": "rm -f .cache/stripe/subscriptions.json",
    "clean:cache-stripe-customers": "rm -f .cache/stripe/customers.json",
    "clean:cache-opening-times": "rm -f .cache/opening-times/*",
    "clean:cache-sumup-products": "rm -f .cache/sumup/sumup-products.json",
    "clean:cache-sumup-customers": "rm -f .cache/sumup/sumup-customers.json",
    "clean:cache-sumup-updates": "rm -f .cache/sumup/sumup-updates.json",
    "clean:cache-sumup-checks": "rm -f .cache/sumup/sumup-check*",
    "clean:cache-sumup-reconcile": "rm -f .cache/sumup/sumup-ashbourne-reconcile.json",
    "build-cache": "run-s build:*",
    "build:cache-ashbourne-csv": "npx cache-ashbourne-csv",
    "build:cache-ashbourne-lint": "npx cache-ashbourne-lint",
    "build:cache-ashbourne-json": "npx cache-ashbourne-json",
    "build:cache-stripe-join": "npx cache-stripe-join",
    "build:cache-stripe-subscriptions": "npx cache-stripe-subscriptions",
    "build:cache-stripe-customers": "npx cache-stripe-customers",
    "build:cache-opening-times": "npx cache-opening-times",
    "build:cache-sumup-products": "npx cache-sumup-products",
    "build:cache-sumup-customers": "npx cache-sumup-customers",
    "build:cache-sumup-updates": "npx cache-sumup-updates",
    "build:cache-sumup-checks": "npx cache-sumup-checks",
    "build:cache-sumup-reconcile": "npx cache-sumup-reconcile",
    "clean-build": "run-s clean-cache build-cache"
  },
  "bin": {
    "cache-stripe-join": "./scripts/cache-stripe-join.js",
    "cache-stripe-subscriptions": "./scripts/cache-stripe-subscriptions.js",
    "cache-stripe-customers": "./scripts/cache-stripe-customers.js",
    "cache-opening-times": "./scripts/cache-opening-times.js",
    "cache-sumup-products": "./scripts/cache-sumup-products.js",
    "cache-sumup-customers": "./scripts/cache-sumup-customers.js",
    "cache-sumup-updates": "./scripts/cache-sumup-updates.js",
    "cache-sumup-checks": "./scripts/cache-sumup-checks.js",
    "cache-sumup-reconcile": "./scripts/cache-sumup-reconcile.js",
    "cache-ashbourne-csv": "./scripts/cache-ashbourne-csv.js",
    "cache-ashbourne-lint": "./scripts/cache-ashbourne-lint.js",
    "cache-ashbourne-json": "./scripts/cache-ashbourne-json.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andystevenson/wwsc.info.git"
  },
  "keywords": [],
  "author": "",
