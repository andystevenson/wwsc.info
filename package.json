  "bugs": {
    "url": "https://github.com/andystevenson/wwsc.info/issues"
  },
  "homepage": "https://github.com/andystevenson/wwsc.info#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-canary.15",
    "@11ty/eleventy-plugin-vite": "^1.0.0",
    "@andystevenson/goodtill": "^0.1.10",
    "@andystevenson/lib": "^0.2.15",
    "@andystevenson/sass": "^0.1.2",
    "@andystevenson/theme-toggle": "^0.1.5",
    "@types/node": "^18.7.18",
    "concurrently": "^7.4.0",
    "csv-parse": "^5.3.0",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.2",
    "download": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "open-props": "^1.4.15",
    "postcss": "^8.4.16",
    "postcss-custom-media": "^8.0.2",
    "postcss-jit-props": "^1.0.7",
    "sass": "^1.54.9",
    "stripe": "^10.10.0",
    "xlsx": "^0.18.5"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7"
  }
